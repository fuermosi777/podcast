<?xml version="1.0" encoding="UTF-8"?>
<rss xmlns:itunes="http://www.itunes.com/dtds/podcast-1.0.dtd" version="2.0">
	<channel>
		<title>
			{{ title }}
		</title>
		<link>
			{{{ link }}}
		</link>
		<description>
			{{ description }}
		</description>
		<language>
			zh-cn
		</language>

		<itunes:summary>{{ summary }}</itunes:summary>
		<itunes:author>{{ author }}</itunes:author>
		<itunes:image href="{{{ image }}}"/>
		<itunes:author>{{ author }}</itunes:author>
		<itunes:owner>
		<itunes:name>fuermosi777</itunes:name>
		<itunes:email>liuhao1990@gmail.com</itunes:email>
		</itunes:owner>

		<pubDate>
			{{ date }}
		</pubDate>

		{{#items}}
			<item>
				<title>
					{{ title }}
				</title>

				<itunes:author>{{ author }}</itunes:author>
				<itunes:subtitle></itunes:subtitle>
				<itunes:summary>
				<![CDATA[ <p> {{ summary }} </p> ]]>
				</itunes:summary>
				<itunes:image href="{{{ image }}}"/>
				<enclosure url="{{{ audio }}}" type="audio/mpeg" />
				<guid isPermaLink="true">
				</guid>
				<duration>
					{{ duration }}
				</duration>
				<pubDate>
					{{ date }}
				</pubDate>
			</item>
		{{/items}}
	</channel>
</rss>
